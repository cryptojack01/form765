          "pdf_field_name": "1.a.",
          "data_path": "personalInfo.lastName",
          "validation": {
            "pattern": "^[A-Za-z\\s\\-']+$",
            "error_message_zh": "姓氏只能包含字母、空格、连字符和撇号",
            "error_message_en": "Last name can only contain letters, spaces, hyphens, and apostrophes"
          }
        },
        {
          "item_number": "1.b.",
          "field_id": "first_name",
